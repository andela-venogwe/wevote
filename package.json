{
  "name": "wevote",
  "version": "1.0.0",
  "engines": {
    "node": ">=9.8.0"
  },
  "description": "A citizens mobilization app",
  "main": "./server/server.js",
  "scripts": {
    "tunnel": "ssh -o ServerAliveInterval=10 -p 443 -R $(git config user.name):80:localhost:4000 serveo.net",
    "build:server": "export NODE_ENV=production && rimraf dist && babel server --out-dir dist/",
    "build:client": "webpack -p",
    "build": "npm run build:server && npm run build:client",
    "start:dev": "export NODE_ENV=development && concurrently \"npm run tunnel\" \"nodemon ./server/server.js\"",
    "start": "export NODE_ENV=production && node dist/server.js",
    "inec:reg": "testcafe chrome:headless ./reg-centers/inec.js",
    "postinstall": "npm run build",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignatiusukwuoma/wevote-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ignatiusukwuoma/wevote-client/issues"
  },
  "homepage": "https://github.com/ignatiusukwuoma/wevote-client/#readme",
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "apollo-boost": "^0.1.10",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-persisted-queries": "^0.2.1",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "axios": "^0.18.0",
    "dataloader": "^1.4.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "google-maps-react": "^2.0.2",
    "graphql": "^0.13.2",
    "graphql-compose": "^3.1.1",
    "graphql-compose-connection": "^3.2.0",
    "graphql-compose-mongoose": "^3.1.3",
    "graphql-compose-pagination": "^3.3.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^2.24.0",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "material-ui-next-community-icons": "^1.0.0",
    "material-ui-pickers": "^1.0.0-rc.11",
    "moment": "^2.22.2",
    "mongoose": "^5.2.3",
    "morgan": "^1.9.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-image": "^1.3.1",
    "react-loader-advanced": "^1.7.1",
    "react-pdf": "^3.0.5",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "recompose": "^0.27.1",
    "serve-favicon": "^2.5.0",
    "subscriptions-transport-ws": "^0.9.12",
    "testcafe": "^0.20.4",
    "validator": "^9.4.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.2",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "dist"
    ],
    "globals": [
      "NODE_ENV",
      "HOST_NAME",
      "WS_HOST_NAME",
      "API_URL",
      "MAPS_KEY",
      "fixture",
      "test"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "test/",
      "client/",
      "dist/",
      "node_modules/"
    ],
    "execMap": {
      "js": "babel-node"
    },
    "delay": "2500"
  }
}
